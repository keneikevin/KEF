<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.KEF" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#2979FF</item>
        <item name="colorPrimaryVariant">#3D5AFE</item>
        <item name="colorOnPrimary">@color/purple_700</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#651FFF</item>
        <item name="colorSecondaryVariant">#3D5AFE</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:navigationBarColor">@color/black</item>

        <!-- Customize your theme here. -->

    </style>

    <style name="Theme.KEF.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.KEF.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.KEF.PopupOverlay" parent="ThemeOverlay.AppCompat.Dark" />
</resources>